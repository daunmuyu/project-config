{
  "name": "@wx/webpack-react",
  "libraryName": "wx",
  "libraryEntry": "index.umd.js",
  "version": "0.0.1",
  "description": "Boilerplate for webpack & react",
  "main": "build/index.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wxyyxc1992/fe-boilerplate"
  },
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "wxyyxc1992@github",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/dev-config/mock/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/dev-config/mock/fileMock.js"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "parallelshell \"npm run mock\" \"npm run dev\"",
    "dev": "webpack-dashboard -- npm run dev:node",
    "dev:node": "webpack-dev-server --config ./dev-config/webpack.config.dev.js --hot",
    "build": "npm run clean && BABEL_ENV=production webpack --config ./dev-config/webpack.config.prod.js --mode production",
    "build:dev": "npm run clean && webpack --config ./dev-config/webpack.config.prod.js --mode development",
    "build:lib": "npm run clean && BABEL_ENV=production webpack --config ./dev-config/webpack.config.umd.js --mode production",
    "stats": "webpack --config ./dev-config/webpack.config.prod.js --mode production --profile --json > build/stats.json",
    "analyze": "npm run stats && webpack-bundle-analyzer build/stats.json",
    "clean": "rimraf build && mkdir build",
    "test": "jest",
    "update": "npm-check -u",
    "lint": "eslint --ext .js,.jsx src test",
    "mock": "json-server --watch dev-config/mock/db.json --port 9091",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "precommit": "lint-staged",
    "prettier-all": "prettier --write 'src/**/*' '!src/{assets,datas}/**'"
  },
  "dependencies": {
    "antd": "^3.1.0",
    "babel-polyfill": "^6.26.0",
    "class-names": "^1.0.0",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "moment": "^2.20.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-promise": "^0.5.3",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.1",
    "@storybook/addon-links": "^3.4.1",
    "@storybook/addons": "^3.4.1",
    "@storybook/react": "^3.4.1",
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "html-webpack-template": "^6.1.0",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "json-server": "^0.12.1",
    "less": "^3.0.1",
    "less-loader": "^4.0.5",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.7.2",
    "npm-check": "^5.6.0",
    "offline-plugin": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "parallelshell": "^3.0.2",
    "postcss-loader": "^2.1.3",
    "react-hot-loader": "^4.0.1",
    "regenerator-runtime": "^0.11.1",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "wasm-loader": "^1.3.0",
    "webpack": "4.4.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.1",
    "workerize-loader": "^1.0.2"
  }
}
